import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scn =new Scanner(System.in);
        int a =scn.nextInt();
        scn.nextLine();
        int s =0;
        for(int i=0;i<a;i++){
            String n =scn.nextLine();
            for(int j=0;j<n.length();j++){
                s+=(int)n.charAt(j);
            }
        }
        System.out.println(s);
    }
}
